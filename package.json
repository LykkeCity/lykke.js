{
  "name": "@lykkex/lykke.js",
  "version": "0.4.8",
  "description": "A TypeScript/Javascript library surrounding Lykke services",
  "keywords": [
    "Lykke"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LykkeCity/lykke.js.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "yarn test && tsc -p . && rollup -c"
  },
  "main": "./dist/bundle/lykke.js",
  "module": "./dist/index.js",
  "jsnext:main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/big.js": "^4.0.5",
    "@types/jest": "^22.2.0",
    "@types/node": "^8.0.31",
    "babel-core": "^6.26.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.5",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^3.0.0",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.1",
    "ts-loader": "^3.5.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.5.0",
    "typescript": "^2.7.2",
    "uglify-es": "^3.3.9"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,scss,sass}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": "tslint --fix"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/src/__tests__/.*)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@types/amplitude-js": "^2.12.5",
    "@types/react": "^16.4.13",
    "amplitude-js": "^4.4.0",
    "big.js": "^5.1.2",
    "react-ga": "^2.5.3"
  }
}
